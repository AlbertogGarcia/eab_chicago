	file = {Sander et al. - 2010 - The value of urban tree cover A hedonic property .pdf:C\:\\Users\\garci\\Zotero\\storage\\6YKFK8W7\\Sander et al. - 2010 - The value of urban tree cover A hedonic property .pdf:application/pdf},
}

