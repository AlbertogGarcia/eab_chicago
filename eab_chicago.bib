	file = {Gidlöf-Gunnarsson and Öhrström - 2007 - Noise and well-being in urban residential environm.pdf:C\:\\Users\\garci\\Zotero\\storage\\7F58Z5DY\\Gidlöf-Gunnarsson and Öhrström - 2007 - Noise and well-being in urban residential environm.pdf:application/pdf},
